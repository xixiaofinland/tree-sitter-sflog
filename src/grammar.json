{
  "name": "sflog",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "log_header"
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "log_entry"
          }
        }
      ]
    },
    "log_header": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "version"
        },
        {
          "type": "SYMBOL",
          "name": "log_level_settings"
        }
      ]
    },
    "version": {
      "type": "PATTERN",
      "value": "\\d+\\.\\d+"
    },
    "log_level_settings": {
      "type": "REPEAT1",
      "content": {
        "type": "SEQ",
        "members": [
          {
            "type": "SYMBOL",
            "name": "component"
          },
          {
            "type": "STRING",
            "value": ","
          },
          {
            "type": "SYMBOL",
            "name": "log_level"
          },
          {
            "type": "CHOICE",
            "members": [
              {
                "type": "STRING",
                "value": ";"
              },
              {
                "type": "BLANK"
              }
            ]
          }
        ]
      }
    },
    "component": {
      "type": "PATTERN",
      "value": "[A-Z_]+"
    },
    "log_level": {
      "type": "PATTERN",
      "value": "[A-Z]+"
    },
    "log_entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "timestamp"
        },
        {
          "type": "STRING",
          "value": "|"
        },
        {
          "type": "SYMBOL",
          "name": "event_identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "|"
                },
                {
                  "type": "SYMBOL",
                  "name": "event_details"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "timestamp": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "time"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "duration"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "time": {
      "type": "PATTERN",
      "value": "\\d{2}:\\d{2}:\\d{2}\\.\\d{1,3}"
    },
    "duration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "event_identifier": {
      "type": "CHOICE",
      "members": [
        {
          "type": "STRING",
          "value": "CALLOUT_REQUEST"
        },
        {
          "type": "STRING",
          "value": "USER_INFO"
        },
        {
          "type": "STRING",
          "value": "EXECUTION_STARTED"
        },
        {
          "type": "STRING",
          "value": "CODE_UNIT_STARTED"
        },
        {
          "type": "STRING",
          "value": "HEAP_ALLOCATE"
        },
        {
          "type": "STRING",
          "value": "VARIABLE_SCOPE_BEGIN"
        },
        {
          "type": "STRING",
          "value": "VARIABLE_ASSIGNMENT"
        },
        {
          "type": "STRING",
          "value": "STATEMENT_EXECUTE"
        },
        {
          "type": "STRING",
          "value": "SYSTEM_METHOD_ENTRY"
        },
        {
          "type": "STRING",
          "value": "SYSTEM_METHOD_EXIT"
        },
        {
          "type": "STRING",
          "value": "SOQL_EXECUTE_BEGIN"
        },
        {
          "type": "STRING",
          "value": "SOQL_EXECUTE_END"
        },
        {
          "type": "STRING",
          "value": "METHOD_ENTRY"
        },
        {
          "type": "STRING",
          "value": "METHOD_EXIT"
        },
        {
          "type": "STRING",
          "value": "CODE_UNIT_FINISHED"
        },
        {
          "type": "STRING",
          "value": "EXECUTION_FINISHED"
        },
        {
          "type": "STRING",
          "value": "CUMULATIVE_LIMIT_USAGE"
        },
        {
          "type": "STRING",
          "value": "CUMULATIVE_LIMIT_USAGE_END"
        }
      ]
    },
    "event_details": {
      "type": "PATTERN",
      "value": ".+"
    },
    "code_location": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "number"
            },
            {
              "type": "STRING",
              "value": "EXTERNAL"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    },
    "string": {
      "type": "PATTERN",
      "value": "[^|]+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
