{
  "name": "salesforce_debug_log",
  "rules": {
    "source_file": {
      "type": "SEQ",
      "members": [
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "log_header"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "REPEAT",
          "content": {
            "type": "SYMBOL",
            "name": "log_entry"
          }
        }
      ]
    },
    "log_header": {
      "type": "SYMBOL",
      "name": "log_level_settings"
    },
    "log_level_settings": {
      "type": "PATTERN",
      "value": ".+"
    },
    "log_entry": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "timestamp"
        },
        {
          "type": "STRING",
          "value": "|"
        },
        {
          "type": "SYMBOL",
          "name": "event_identifier"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SEQ",
              "members": [
                {
                  "type": "STRING",
                  "value": "|"
                },
                {
                  "type": "SYMBOL",
                  "name": "event_details"
                }
              ]
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "timestamp": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "time"
        },
        {
          "type": "SYMBOL",
          "name": "duration"
        }
      ]
    },
    "time": {
      "type": "PATTERN",
      "value": "\\d{2}:\\d{2}:\\d{2}\\.\\d{3}"
    },
    "duration": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "("
        },
        {
          "type": "SYMBOL",
          "name": "number"
        },
        {
          "type": "STRING",
          "value": ")"
        }
      ]
    },
    "event_identifier": {
      "type": "SEQ",
      "members": [
        {
          "type": "SYMBOL",
          "name": "event_name"
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "line_number"
            },
            {
              "type": "BLANK"
            }
          ]
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "log_level"
            },
            {
              "type": "BLANK"
            }
          ]
        }
      ]
    },
    "event_name": {
      "type": "PATTERN",
      "value": "[A-Z_]+"
    },
    "line_number": {
      "type": "SEQ",
      "members": [
        {
          "type": "STRING",
          "value": "["
        },
        {
          "type": "CHOICE",
          "members": [
            {
              "type": "SYMBOL",
              "name": "number"
            },
            {
              "type": "STRING",
              "value": "EXTERNAL"
            }
          ]
        },
        {
          "type": "STRING",
          "value": "]"
        }
      ]
    },
    "log_level": {
      "type": "PATTERN",
      "value": "[A-Z]+"
    },
    "event_details": {
      "type": "PATTERN",
      "value": ".+"
    },
    "number": {
      "type": "PATTERN",
      "value": "\\d+"
    }
  },
  "extras": [
    {
      "type": "PATTERN",
      "value": "\\s"
    }
  ],
  "conflicts": [],
  "precedences": [],
  "externals": [],
  "inline": [],
  "supertypes": []
}
